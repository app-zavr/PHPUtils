{
  "name": "appzavr/php-utils",
  "type": "library",
  "description": "A set of tools for PHP by Appzavr",
  "license": "MIT",
  "require": {
    "php": ">=5.4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "5.7.4"
  },
  "autoload": {
    "psr-4": {
      "Appzavr\\PHPUtils\\": "src/"
    },
    "files": [
      "src/Lang.php",
      "src/Random.php",
      "src/Response.php",
      "src/StringNumbers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Appzavr\\PHPUtils\\Tests\\": "tests/"
    }
  }
}
